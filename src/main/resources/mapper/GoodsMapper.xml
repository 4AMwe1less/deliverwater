<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.deliver.water.mapper.GoodsMapper">

    <select id="getAllGoods" resultType="cn.deliver.water.entity.Goods">
        select * from goods
    </select>
    <select id="getAllEvalute" resultType="cn.deliver.water.entity.Evaluate">
        select * from evaluate where gid=#{gid}
    </select>
</mapper>